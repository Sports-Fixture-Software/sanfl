{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "build",
    "moduleResolution": "node",
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "exclude": [
    "node_modules"  
  ]
}
